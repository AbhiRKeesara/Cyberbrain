{
	"name": "cyberbrain",
	"displayName": "Cyberbrain",
	"description": "Python debugging, redefined.",
	"version": "0.1.0",
	"publisher": "laike9m",
	"repository": "https://github.com/laike9m/Cyberbrain",
	"license": "MIT",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Debuggers"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"unittest": "mocha",
		"copy": "node node_modules/rimraf/bin.js ./out/*.js && node node_modules/copyfiles/copyfiles -u 1 ./src/*.js ./out/",
		"vscode:prepublish": "npm run compile",
		"compile": "npm run copy && tsc -p ./",
		"lint": "eslint src --ext ts --fix",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/google-protobuf": "^3.7.4",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.33",
		"@types/vscode": "^1.51.0",
		"@typescript-eslint/eslint-plugin": "^4.8.2",
		"@typescript-eslint/parser": "^4.8.2",
		"copyfiles": "^2.4.1",
		"glob": "^7.1.6",
		"hamjest": "^3.7.1",
		"mocha": "^7.2.0",
		"prettier-eslint-cli": "^5.0.0",
		"ts-protoc-gen": "^0.12.0",
		"typescript": "^3.9.7",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"@grpc/grpc-js": "^1.2.1",
		"google-auth-library": "^6.1.3",
		"google-protobuf": "^3.14.0",
		"grpc-tools": "^1.10.0",
		"log4js": "^6.3.0",
		"randomcolor": "^0.6.2",
		"vis-network": "^8.5.4"
	},
	"type": "module"
}
